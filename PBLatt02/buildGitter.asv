function Omega = buildGitter(h)
    Omega = zeros(3/h+1,3/h+1,2);
    for i=1:3/h
        for j=1:3/h
            Omega(i,j,1) = (i-1)*h;
            Omega(i,j,2) = (j-1)*h;
            
            if i*h>2 && j*h < 1
                Omega(i,j,1) = Nan;
                Omega(i,j,2) = Nan;
            end 
            
            if i*h<1 && j*h > 2
                Omega(i,j,1) = Nan;
                Omega(i,j,2) = Nan;
            end 
        end 
    end 
    Omega
end 
